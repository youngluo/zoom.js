{
  "name": "@youngluo/zoom.js",
  "version": "1.0.0",
  "description": "Medium.com's image zoom with no dependencies",
  "main": "dist/zoom.umd.min.js",
  "module": "dist/zoom.es.js",
  "scripts": {
    "umd": "rollup -c rollup.config.umd.js",
    "umdProd": "rollup -c rollup.config.umd.js --environment build:production",
    "iife": "rollup -c rollup.config.iife.js",
    "iifeProd": "rollup -c rollup.config.iife.js --environment build:production",
    "es": "rollup -c rollup.config.es.js",
    "start": "npm run iife && npm run iifeProd && npm run umd && npm run umdProd && npm run es"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/youngluo/zoom.js.git"
  },
  "keywords": [
    "zoom",
    "image"
  ],
  "author": "Young Luo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/youngluo/zoom.js/issues"
  },
  "homepage": "https://github.com/youngluo/zoom.js",
  "devDependencies": {
    "babel-preset-latest": "^6.24.1",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-conditional": "^1.1.1",
    "rollup-plugin-license": "^0.4.0",
    "rollup-plugin-uglify": "^2.0.1",
    "uglify-es": "^3.0.15"
  }
}
